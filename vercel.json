{
    "version": 2,

    "functions": {
        "app/api/execute/route.py": {
          "memory": 256,
          "maxDuration": 10,
          "runtime": "@vercel/python@3.1.0"
        }
      },
      
    "builds": [
        {
            "src": "app/api/execute/route.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/execute",
            "methods": ["POST", "OPTIONS"],
            "dest": "app/api/execute/route.py"
        },
        {
            "src": "/((?!api/execute).*)",
            "dest": "/"
        }
    ]
}